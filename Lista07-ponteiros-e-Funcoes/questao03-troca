// NÃO ALTERE AS LINHAS ABAIXO
#include <stdio.h>

void troca(int *x, int *y);

int main(void)
{
	int N;
	int a, b;

	scanf("%d", &N);
	for (int i = 0; i < N; i++) {
		scanf("%d%d", &a, &b);
		troca(&a, &b);
		printf("%d %d\n", a, b);
	}
}
// NÃO ALTERE AS LINHAS ACIMA

/* Não altere o protótipo da função. Apenas complete
 * o corpo da função.
 */
void troca(int *x, int *y)
{
	int temp;
	
	temp = *x;
	*x = *y;
	*y = temp;
}
